{
  "version": 2,
  "builds": [
    {
      "src": "dentaltrawler/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/dentaltrawler/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/dentaltrawler/$1"
    }
  ]
}

