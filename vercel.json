{
  "version": 2,
  "builds": [
    {
      "src": "dentaltrawler/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dentaltrawler/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/dentaltrawler/$1"
    },
    {
      "source": "/((?!dentaltrawler/).*)",
      "destination": "/dentaltrawler/index.html"
    }
  ]
}

